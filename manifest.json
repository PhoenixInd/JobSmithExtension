{
    "manifest_version": 3,
    "name": "JobSmith",
    "version": "0.1",
    "description": "JobSmith extention",
    "icons": {
        "128": "icon.png"
    },
    "action":{
        "default_title": "JobSmith",
        "default_popup": "popup/index.html"
    },
    "permissions":[
        "scripting",
        "activeTab",
        "storage",
        "tabs",
        "webRequest"
    ],
    "background":{
        "service_worker": "./scripts/background.js",
        "type": "module"
    },
    "host_permissions": [
        "*://*.linkedin.com/*"
    ]
}